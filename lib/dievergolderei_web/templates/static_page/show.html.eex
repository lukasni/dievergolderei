<h1>Show Static page</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @static_page.name %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @static_page.content %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.static_page_path(@conn, :edit, @static_page) %></span>
<span><%= link "Back", to: Routes.static_page_path(@conn, :index) %></span>
